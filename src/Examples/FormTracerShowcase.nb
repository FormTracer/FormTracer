   RowBox[{"sp", "[", 
    RowBox[{"p", ",", "q"}], "]"}], ",", 
   RowBox[{"eps", "[", "]"}], ",", 
   RowBox[{"deltaDirac", "[", 
    RowBox[{"i", ",", "j"}], "]"}], ",", 
   RowBox[{"gamma", "[", 
    RowBox[{"mu", ",", "i", ",", "j"}], "]"}], ",", 
   RowBox[{"gamma5", "[", 
